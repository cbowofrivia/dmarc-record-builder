{
    "name": "cbowofrivia/dmarc-record-builder",
    "description": "A lean package that makes creating DMARC records user friendly",
    "keywords": [
        "cbowofrivia",
        "dmarc-record-builder",
        "dmarc"
    ],
    "homepage": "https://github.com/cbowofrivia/dmarc-record-builder",
    "license": "MIT",
    "authors": [
        {
            "name": "Charles Bowen",
            "email": "charlesrbowen93@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/collections": "^12.0",
        "webmozart/assert": "^1.10"
    },
    "require-dev": {
        "laravel/pint": "^1.15",
        "symfony/var-dumper": "^7.0",
        "pestphp/pest": "^4"
    },
    "autoload": {
        "psr-4": {
            "CbowOfRivia\\DmarcRecordBuilder\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CbowOfRivia\\DmarcRecordBuilder\\Tests\\": "tests"
        },
        "files": [
            "vendor/illuminate/collections/helpers.php"
        ]
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
